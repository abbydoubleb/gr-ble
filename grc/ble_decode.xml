<?xml version="1.0"?>
<block>
  <name>decode</name>
  <key>ble_decode</key>
  <category>[ble]</category>
  <import>import ble</import>
  <make>ble.decode($channel)</make>
	<callback>set_channel($channel)</callback>
  <param>
    <name>Channel</name>
    <key>channel</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
</block>
